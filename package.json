{
	"name": "@yorganci/npm-registry-api",
	"version": "0.2.0",
	"description": "Fully typesafe npm registry API client with optional caching.",
	"license": "MIT",
	"author": {
		"name": "Atahan Yorganci",
		"url": "https://github.com/atahanyorganci"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/atahanyorganci/npm-registry-api.git"
	},
	"bugs": {
		"url": "https://github.com/atahanyorganci/npm-registry-api/issues"
	},
	"keywords": [
		"npm",
		"registry",
		"typescript",
		"isomorphic-fetch",
		"npm-registry",
		"npm-search",
		"npm-downloads",
		"packument",
		"npm-registry-api",
		"zod-validation "
	],
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.ts"
		},
		"./cache": {
			"import": "./dist/cache.mjs",
			"require": "./dist/cache.cjs",
			"types": "./dist/cache.d.ts"
		},
		"./package.json": "./package.json"
	},
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"files": ["dist"],
	"scripts": {
		"attw": "attw --pack . --ignore-rules no-resolution",
		"build": "unbuild",
		"check": "tsc",
		"check:watch": "tsc --watch",
		"format": "biome format .",
		"format:write": "biome format --write .",
		"lint": "biome check .",
		"lint:write": "biome check --write .",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"test:ci": "vitest run --coverage --bail 1"
	},
	"dependencies": {
		"ofetch": "^1.3.4",
		"ohash": "^1.1.3",
		"unstorage": "^1.10.2",
		"zod": "^3.23.8",
		"zod-package-json": "^1.0.3"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.15.3",
		"@biomejs/biome": "1.8.3",
		"@microsoft/api-extractor": "^7.47.4",
		"@types/bun": "^1.1.6",
		"@vitest/coverage-v8": "^2.0.3",
		"fflate": "^0.8.2",
		"typescript": "^5.5.3",
		"unbuild": "^2.0.0",
		"vitest": "^2.0.3"
	}
}
